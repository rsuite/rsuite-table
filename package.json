    "react-code-view": "^2.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "rsuite": "5.78.0",
    "sinon": "^19.0.2",
    "sinon-chai": "^3.7.0",
    "size-limit": "^11.1.6",
    "style-loader": "^0.13.1",
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.18.2",
