    "autoprefixer": "^8.3.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^4.3.5",
    "conventional-changelog-cli": "^2.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^5.2.1",
    "css-loader": "^0.23.1",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
